{
  "name": "@sandstorm/adapters-e2b",
  "version": "0.0.0",
  "description": "E2B adapter for Sandstorm",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "bun ../../scripts/build.ts --format both",
    "dev": "bun --watch ../../scripts/build.ts --format both",
    "test": "bun test",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@e2b/code-interpreter": "^1.0.2",
    "@sandstorm/core": "workspace:*",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@sandstorm/test-utils": "workspace:*",
    "typescript": "^5.7.2",
    "bun-types": "^1.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}
